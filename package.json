{
  "name": "@alex.radulescu/styled-static",
  "version": "0.7.5",
  "description": "Near-zero-runtime styled components for React 19+ with Vite. CSS extracted at build time, minimal runtime.",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && bun run build:minify && bun run build:sizes",
    "build:minify": "bun scripts/minify.ts",
    "build:sizes": "bun scripts/sizes.ts",
    "dev": "vite",
    "dev:watch": "tsc --watch",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "react": "^19.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^4.0.16",
    "@vitest/coverage-v8": "2.1.8",
    "acorn": "^8.15.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "styled-components",
    "css-in-js",
    "vite",
    "react",
    "zero-runtime",
    "static-css"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexradulescu/styled-static"
  },
  "author": "Alex Radulescu"
}
